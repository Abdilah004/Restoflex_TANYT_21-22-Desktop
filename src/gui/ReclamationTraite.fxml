<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ReclamationTraiteController">
      <children>
      <Label layoutX="381.0" layoutY="35.0" text="Réclamation" textFill="#1e85f1">
         <font>
            <Font name="Cambria Bold" size="35.0" />
         </font>
      </Label>
      <Button fx:id="IdBtnSupp" layoutX="714.0" layoutY="178.0" mnemonicParsing="false" onAction="#BtnSuppRec" stylesheets="@StyleAjout.css" text="Supprimer">
         <font>
            <Font name="Cambria" size="14.0" />
         </font></Button>
      <TableView fx:id="tableRec" layoutX="242.0" layoutY="225.0" prefHeight="300.0" prefWidth="600.0" stylesheets="@StyleTable.css">
        <columns>
          <TableColumn fx:id="ColDest" prefWidth="219.0" style="-fx-background-color: white;" text="Destinataire" />
          <TableColumn fx:id="ColStat" prefWidth="183.0" style="-fx-background-color: white;" text="Statut" />
            <TableColumn fx:id="ColDate" minWidth="0.0" prefWidth="196.0" style="-fx-background-color: white;" text="Date" />
        </columns>
      </TableView>
      <DatePicker fx:id="recherche" layoutX="242.0" layoutY="180.0" onAction="#recherche" style="-fx-background-color: transparent; -fx-border-radius: 8px; -fx-border-color: #1e85f1;" />
      <Button fx:id="btnretour" layoutX="22.0" layoutY="19.0" mnemonicParsing="false" onAction="#retour" stylesheets="@StyleAjout.css" text="Retour">
         <font>
            <Font name="Cambria" size="14.0" />
         </font>
      </Button>
      <Button fx:id="btnDetails" layoutX="497.0" layoutY="544.0" mnemonicParsing="false" onAction="#details" stylesheets="@StyleAjout.css" text="Détails">
         <font>
            <Font name="Cambria" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
