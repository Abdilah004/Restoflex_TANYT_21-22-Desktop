      <Button fx:id="btn_add_res" layoutX="826.0" layoutY="134.0" mnemonicParsing="false" onAction="#show_add_rest" prefHeight="40.0" prefWidth="261.0" styleClass="blue-btn" text="+ Ajouter nouveau Restaurant">
